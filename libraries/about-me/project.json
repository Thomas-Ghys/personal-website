{
	"name": "about-me",
	"$schema": "../node_modules/nx/schemas/project-schema.json",
	"sourceRoot": "about-me/src",
	"prefix": "personal-website",
	"tags": [],
	"projectType": "library",
	"targets": {
		"build": {
			"executor": "@nx/angular:ng-packagr-lite",
			"outputs": ["{workspaceRoot}/dist/{projectRoot}"],
			"options": {
				"project": "about-me/ng-package.json"
			},
			"configurations": {
				"production": {
					"tsConfig": "about-me/tsconfig.lib.prod.json"
				},
				"development": {
					"tsConfig": "about-me/tsconfig.lib.json"
				}
			},
			"defaultConfiguration": "production"
		},
		"test": {
			"executor": "@nx/jest:jest",
			"outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
			"options": {
				"jestConfig": "about-me/jest.config.ts",
				"passWithNoTests": true
			},
			"configurations": {
				"ci": {
					"ci": true,
					"codeCoverage": true
				}
			}
		},
		"lint": {
			"executor": "@nx/eslint:lint",
			"outputs": ["{options.outputFile}"],
			"options": {
				"lintFilePatterns": [
					"about-me/**/*.ts",
					"about-me/**/*.html",
					"about-me/package.json"
				]
			}
		}
	}
}
